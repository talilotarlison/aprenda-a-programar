//detectar numeros impares ate 16:
//Metodo com While
var i = 1

while (i <= 16) {
 i++;
 
 if (i % 2 == 0){
     console.log(`O numero ${i} é impar!!`);
}

 }
 
 //metodo com For:
 
 

for ( i=1 ;  i <= 16; i++) {
 
 if (i % 2 !== 0){
     console.log(`O numero ${i} é impar!!`);
}

 }